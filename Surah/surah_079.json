        {
            "index": "079",
            "name": "an-Nazi'at",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "وَالنَّازِعَاتِ غَرْقًا",
                "verse_2": "وَالنَّاشِطَاتِ نَشْطًا",
                "verse_3": "وَالسَّابِحَاتِ سَبْحًا",
                "verse_4": "فَالسَّابِقَاتِ سَبْقًا",
                "verse_5": "فَالْمُدَبِّرَاتِ أَمْرًا",
                "verse_6": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ",
                "verse_7": "تَتْبَعُهَا الرَّادِفَةُ",
                "verse_8": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
                "verse_9": "أَبْصَارُهَا خَاشِعَةٌ",
                "verse_10": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ",
                "verse_11": "أَإِذَا كُنَّا عِظَامًا نَخِرَةً",
                "verse_12": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
                "verse_13": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ",
                "verse_14": "فَإِذَا هُمْ بِالسَّاهِرَةِ",
                "verse_15": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ",
                "verse_16": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى",
                "verse_17": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ",
                "verse_18": "فَقُلْ هَلْ لَكَ إِلَىٰ أَنْ تَزَكَّىٰ",
                "verse_19": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
                "verse_20": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ",
                "verse_21": "فَكَذَّبَ وَعَصَىٰ",
                "verse_22": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
                "verse_23": "فَحَشَرَ فَنَادَىٰ",
                "verse_24": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ",
                "verse_25": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ",
                "verse_26": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِمَنْ يَخْشَىٰ",
                "verse_27": "أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا",
                "verse_28": "رَفَعَ سَمْكَهَا فَسَوَّاهَا",
                "verse_29": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا",
                "verse_30": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا",
                "verse_31": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا",
                "verse_32": "وَالْجِبَالَ أَرْسَاهَا",
                "verse_33": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ",
                "verse_34": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ",
                "verse_35": "يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَىٰ",
                "verse_36": "وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَىٰ",
                "verse_37": "فَأَمَّا مَنْ طَغَىٰ",
                "verse_38": "وَآثَرَ الْحَيَاةَ الدُّنْيَا",
                "verse_39": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ",
                "verse_40": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ",
                "verse_41": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ",
                "verse_42": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا",
                "verse_43": "فِيمَ أَنْتَ مِنْ ذِكْرَاهَا",
                "verse_44": "إِلَىٰ رَبِّكَ مُنْتَهَاهَا",
                "verse_45": "إِنَّمَا أَنْتَ مُنْذِرُ مَنْ يَخْشَاهَا",
                "verse_46": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"
            },
