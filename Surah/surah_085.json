            "name": "al-Buruj",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ",
                "verse_2": "وَالْيَوْمِ الْمَوْعُودِ",
                "verse_3": "وَشَاهِدٍ وَمَشْهُودٍ",
                "verse_4": "قُتِلَ أَصْحَابُ الْأُخْدُودِ",
                "verse_5": "النَّارِ ذَاتِ الْوَقُودِ",
                "verse_6": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
                "verse_7": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ",
                "verse_8": "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَنْ يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ",
                "verse_9": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ",
                "verse_10": "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ",
                "verse_11": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ",
                "verse_12": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
                "verse_13": "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ",
                "verse_14": "وَهُوَ الْغَفُورُ الْوَدُودُ",
                "verse_15": "ذُو الْعَرْشِ الْمَجِيدُ",
                "verse_16": "فَعَّالٌ لِمَا يُرِيدُ",
                "verse_17": "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ",
                "verse_18": "فِرْعَوْنَ وَثَمُودَ",
                "verse_19": "بَلِ الَّذِينَ كَفَرُوا فِي تَكْذِيبٍ",
                "verse_20": "وَاللَّهُ مِنْ وَرَائِهِمْ مُحِيطٌ",
                "verse_21": "بَلْ هُوَ قُرْآنٌ مَجِيدٌ",
                "verse_22": "فِي لَوْحٍ مَحْفُوظٍ"
            },
