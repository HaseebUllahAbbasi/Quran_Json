        {
            "index": "089",
            "name": "al-Fajr",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "وَالْفَجْرِ",
                "verse_2": "وَلَيَالٍ عَشْرٍ",
                "verse_3": "وَالشَّفْعِ وَالْوَتْرِ",
                "verse_4": "وَاللَّيْلِ إِذَا يَسْرِ",
                "verse_5": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِذِي حِجْرٍ",
                "verse_6": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
                "verse_7": "إِرَمَ ذَاتِ الْعِمَادِ",
                "verse_8": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ",
                "verse_9": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ",
                "verse_10": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ",
                "verse_11": "الَّذِينَ طَغَوْا فِي الْبِلَادِ",
                "verse_12": "فَأَكْثَرُوا فِيهَا الْفَسَادَ",
                "verse_13": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
                "verse_14": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ",
                "verse_15": "فَأَمَّا الْإِنْسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ",
                "verse_16": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ",
                "verse_17": "كَلَّا ۖ بَلْ لَا تُكْرِمُونَ الْيَتِيمَ",
                "verse_18": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ",
                "verse_19": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَمًّا",
                "verse_20": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا",
                "verse_21": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا",
                "verse_22": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا",
                "verse_23": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنْسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ",
                "verse_24": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي",
                "verse_25": "فَيَوْمَئِذٍ لَا يُعَذِّبُ عَذَابَهُ أَحَدٌ",
                "verse_26": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ",
                "verse_27": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ",
                "verse_28": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَرْضِيَّةً",
                "verse_29": "فَادْخُلِي فِي عِبَادِي",
                "verse_30": "وَادْخُلِي جَنَّتِي"
            },
