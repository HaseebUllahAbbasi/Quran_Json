            "name": "at-Takwir",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "إِذَا الشَّمْسُ كُوِّرَتْ",
                "verse_2": "وَإِذَا النُّجُومُ انْكَدَرَتْ",
                "verse_3": "وَإِذَا الْجِبَالُ سُيِّرَتْ",
                "verse_4": "وَإِذَا الْعِشَارُ عُطِّلَتْ",
                "verse_5": "وَإِذَا الْوُحُوشُ حُشِرَتْ",
                "verse_6": "وَإِذَا الْبِحَارُ سُجِّرَتْ",
                "verse_7": "وَإِذَا النُّفُوسُ زُوِّجَتْ",
                "verse_8": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ",
                "verse_9": "بِأَيِّ ذَنْبٍ قُتِلَتْ",
                "verse_10": "وَإِذَا الصُّحُفُ نُشِرَتْ",
                "verse_11": "وَإِذَا السَّمَاءُ كُشِطَتْ",
                "verse_12": "وَإِذَا الْجَحِيمُ سُعِّرَتْ",
                "verse_13": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ",
                "verse_14": "عَلِمَتْ نَفْسٌ مَا أَحْضَرَتْ",
                "verse_15": "فَلَا أُقْسِمُ بِالْخُنَّسِ",
                "verse_16": "الْجَوَارِ الْكُنَّسِ",
                "verse_17": "وَاللَّيْلِ إِذَا عَسْعَسَ",
                "verse_18": "وَالصُّبْحِ إِذَا تَنَفَّسَ",
                "verse_19": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ",
                "verse_20": "ذِي قُوَّةٍ عِنْدَ ذِي الْعَرْشِ مَكِينٍ",
                "verse_21": "مُطَاعٍ ثَمَّ أَمِينٍ",
                "verse_22": "وَمَا صَاحِبُكُمْ بِمَجْنُونٍ",
                "verse_23": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ",
                "verse_24": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ",
                "verse_25": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَجِيمٍ",
                "verse_26": "فَأَيْنَ تَذْهَبُونَ",
                "verse_27": "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ",
                "verse_28": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَسْتَقِيمَ",
                "verse_29": "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ"
            },
