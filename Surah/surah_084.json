            "name": "Al-Inshiqaq",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "إِذَا السَّمَاءُ انْشَقَّتْ",
                "verse_2": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
                "verse_3": "وَإِذَا الْأَرْضُ مُدَّتْ",
                "verse_4": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ",
                "verse_5": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
                "verse_6": "يَا أَيُّهَا الْإِنْسَانُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَاقِيهِ",
                "verse_7": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ",
                "verse_8": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا",
                "verse_9": "وَيَنْقَلِبُ إِلَىٰ أَهْلِهِ مَسْرُورًا",
                "verse_10": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ وَرَاءَ ظَهْرِهِ",
                "verse_11": "فَسَوْفَ يَدْعُو ثُبُورًا",
                "verse_12": "وَيَصْلَىٰ سَعِيرًا",
                "verse_13": "إِنَّهُ كَانَ فِي أَهْلِهِ مَسْرُورًا",
                "verse_14": "إِنَّهُ ظَنَّ أَنْ لَنْ يَحُورَ",
                "verse_15": "بَلَىٰ إِنَّ رَبَّهُ كَانَ بِهِ بَصِيرًا",
                "verse_16": "فَلَا أُقْسِمُ بِالشَّفَقِ",
                "verse_17": "وَاللَّيْلِ وَمَا وَسَقَ",
                "verse_18": "وَالْقَمَرِ إِذَا اتَّسَقَ",
                "verse_19": "لَتَرْكَبُنَّ طَبَقًا عَنْ طَبَقٍ",
                "verse_20": "فَمَا لَهُمْ لَا يُؤْمِنُونَ",
                "verse_21": "وَإِذَا قُرِئَ عَلَيْهِمُ الْقُرْآنُ لَا يَسْجُدُونَ ۩",
                "verse_22": "بَلِ الَّذِينَ كَفَرُوا يُكَذِّبُونَ",
                "verse_23": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَ",
                "verse_24": "فَبَشِّرْهُمْ بِعَذَابٍ أَلِيمٍ",
                "verse_25": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"
            },
