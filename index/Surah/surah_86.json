            "name": "At-Tariq",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "وَالسَّمَاءِ وَالطَّارِقِ",
                "verse_2": "وَمَا أَدْرَاكَ مَا الطَّارِقُ",
                "verse_3": "النَّجْمُ الثَّاقِبُ",
                "verse_4": "إِنْ كُلُّ نَفْسٍ لَمَّا عَلَيْهَا حَافِظٌ",
                "verse_5": "فَلْيَنْظُرِ الْإِنْسَانُ مِمَّ خُلِقَ",
                "verse_6": "خُلِقَ مِنْ مَاءٍ دَافِقٍ",
                "verse_7": "يَخْرُجُ مِنْ بَيْنِ الصُّلْبِ وَالتَّرَائِبِ",
                "verse_8": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ",
                "verse_9": "يَوْمَ تُبْلَى السَّرَائِرُ",
                "verse_10": "فَمَا لَهُ مِنْ قُوَّةٍ وَلَا نَاصِرٍ",
                "verse_11": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ",
                "verse_12": "وَالْأَرْضِ ذَاتِ الصَّدْعِ",
                "verse_13": "إِنَّهُ لَقَوْلٌ فَصْلٌ",
                "verse_14": "وَمَا هُوَ بِالْهَزْلِ",
                "verse_15": "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
                "verse_16": "وَأَكِيدُ كَيْدًا",
                "verse_17": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا"
            },
