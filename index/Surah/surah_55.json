            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "الرَّحْمَٰنُ",
                "verse_2": "عَلَّمَ الْقُرْآنَ",
                "verse_3": "خَلَقَ الْإِنْسَانَ",
                "verse_4": "عَلَّمَهُ الْبَيَانَ",
                "verse_5": "الشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ",
                "verse_6": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ",
                "verse_7": "وَالسَّمَاءَ رَفَعَهَا وَوَضَعَ الْمِيزَانَ",
                "verse_8": "أَلَّا تَطْغَوْا فِي الْمِيزَانِ",
                "verse_9": "وَأَقِيمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيزَانَ",
                "verse_10": "وَالْأَرْضَ وَضَعَهَا لِلْأَنَامِ",
                "verse_11": "فِيهَا فَاكِهَةٌ وَالنَّخْلُ ذَاتُ الْأَكْمَامِ",
                "verse_12": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُ",
                "verse_13": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_14": "خَلَقَ الْإِنْسَانَ مِنْ صَلْصَالٍ كَالْفَخَّارِ",
                "verse_15": "وَخَلَقَ الْجَانَّ مِنْ مَارِجٍ مِنْ نَارٍ",
                "verse_16": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_17": "رَبُّ الْمَشْرِقَيْنِ وَرَبُّ الْمَغْرِبَيْنِ",
                "verse_18": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_19": "مَرَجَ الْبَحْرَيْنِ يَلْتَقِيَانِ",
                "verse_20": "بَيْنَهُمَا بَرْزَخٌ لَا يَبْغِيَانِ",
                "verse_21": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_22": "يَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ",
                "verse_23": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_24": "وَلَهُ الْجَوَارِ الْمُنْشَآتُ فِي الْبَحْرِ كَالْأَعْلَامِ",
                "verse_25": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_26": "كُلُّ مَنْ عَلَيْهَا فَانٍ",
                "verse_27": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو الْجَلَالِ وَالْإِكْرَامِ",
                "verse_28": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_29": "يَسْأَلُهُ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِي شَأْنٍ",
                "verse_30": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_31": "سَنَفْرُغُ لَكُمْ أَيُّهَ الثَّقَلَانِ",
                "verse_32": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_33": "يَا مَعْشَرَ الْجِنِّ وَالْإِنْسِ إِنِ اسْتَطَعْتُمْ أَنْ تَنْفُذُوا مِنْ أَقْطَارِ السَّمَاوَاتِ وَالْأَرْضِ فَانْفُذُوا ۚ لَا تَنْفُذُونَ إِلَّا بِسُلْطَانٍ",
                "verse_34": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_35": "يُرْسَلُ عَلَيْكُمَا شُوَاظٌ مِنْ نَارٍ وَنُحَاسٌ فَلَا تَنْتَصِرَانِ",
                "verse_36": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_37": "فَإِذَا انْشَقَّتِ السَّمَاءُ فَكَانَتْ وَرْدَةً كَالدِّهَانِ",
                "verse_38": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_39": "فَيَوْمَئِذٍ لَا يُسْأَلُ عَنْ ذَنْبِهِ إِنْسٌ وَلَا جَانٌّ",
                "verse_40": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_41": "يُعْرَفُ الْمُجْرِمُونَ بِسِيمَاهُمْ فَيُؤْخَذُ بِالنَّوَاصِي وَالْأَقْدَامِ",
                "verse_42": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_43": "هَٰذِهِ جَهَنَّمُ الَّتِي يُكَذِّبُ بِهَا الْمُجْرِمُونَ",
                "verse_44": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ آنٍ",
                "verse_45": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_46": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِ جَنَّتَانِ",
                "verse_47": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_48": "ذَوَاتَا أَفْنَانٍ",
                "verse_49": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_50": "فِيهِمَا عَيْنَانِ تَجْرِيَانِ",
                "verse_51": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_52": "فِيهِمَا مِنْ كُلِّ فَاكِهَةٍ زَوْجَانِ",
                "verse_53": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_54": "مُتَّكِئِينَ عَلَىٰ فُرُشٍ بَطَائِنُهَا مِنْ إِسْتَبْرَقٍ ۚ وَجَنَى الْجَنَّتَيْنِ دَانٍ",
                "verse_55": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_56": "فِيهِنَّ قَاصِرَاتُ الطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنْسٌ قَبْلَهُمْ وَلَا جَانٌّ",
                "verse_57": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
                "verse_58": "كَأَنَّهُنَّ الْيَاقُوتُ وَالْمَرْجَانُ",
                "verse_59": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
