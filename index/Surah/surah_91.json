            "index": "091",
            "name": "Ash-Shams",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "وَالشَّمْسِ وَضُحَاهَا",
                "verse_2": "وَالْقَمَرِ إِذَا تَلَاهَا",
                "verse_3": "وَالنَّهَارِ إِذَا جَلَّاهَا",
                "verse_4": "وَاللَّيْلِ إِذَا يَغْشَاهَا",
                "verse_5": "وَالسَّمَاءِ وَمَا بَنَاهَا",
                "verse_6": "وَالْأَرْضِ وَمَا طَحَاهَا",
                "verse_7": "وَنَفْسٍ وَمَا سَوَّاهَا",
                "verse_8": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا",
                "verse_9": "قَدْ أَفْلَحَ مَنْ زَكَّاهَا",
                "verse_10": "وَقَدْ خَابَ مَنْ دَسَّاهَا",
                "verse_11": "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا",
                "verse_12": "إِذِ انْبَعَثَ أَشْقَاهَا",
                "verse_13": "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا",
                "verse_14": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُمْ بِذَنْبِهِمْ فَسَوَّاهَا",
                "verse_15": "وَلَا يَخَافُ عُقْبَاهَا"
            },
            "count": 15,
