            "name": "ad-Dhuha",
            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "وَالضُّحَىٰ",
                "verse_2": "وَاللَّيْلِ إِذَا سَجَىٰ",
                "verse_3": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
                "verse_4": "وَلَلْآخِرَةُ خَيْرٌ لَكَ مِنَ الْأُولَىٰ",
                "verse_5": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰ",
                "verse_6": "أَلَمْ يَجِدْكَ يَتِيمًا فَآوَىٰ",
                "verse_7": "وَوَجَدَكَ ضَالًّا فَهَدَىٰ",
                "verse_8": "وَوَجَدَكَ عَائِلًا فَأَغْنَىٰ",
                "verse_9": "فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ",
                "verse_10": "وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ",
                "verse_11": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ"
            },
