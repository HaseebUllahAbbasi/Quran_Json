            "verse": {
                "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "verse_1": "سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى",
                "verse_2": "الَّذِي خَلَقَ فَسَوَّىٰ",
                "verse_3": "وَالَّذِي قَدَّرَ فَهَدَىٰ",
                "verse_4": "وَالَّذِي أَخْرَجَ الْمَرْعَىٰ",
                "verse_5": "فَجَعَلَهُ غُثَاءً أَحْوَىٰ",
                "verse_6": "سَنُقْرِئُكَ فَلَا تَنْسَىٰ",
                "verse_7": "إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ",
                "verse_8": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
                "verse_9": "فَذَكِّرْ إِنْ نَفَعَتِ الذِّكْرَىٰ",
                "verse_10": "سَيَذَّكَّرُ مَنْ يَخْشَىٰ",
                "verse_11": "وَيَتَجَنَّبُهَا الْأَشْقَى",
                "verse_12": "الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ",
                "verse_13": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
                "verse_14": "قَدْ أَفْلَحَ مَنْ تَزَكَّىٰ",
                "verse_15": "وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ",
                "verse_16": "بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا",
                "verse_17": "وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ",
                "verse_18": "إِنَّ هَٰذَا لَفِي الصُّحُفِ الْأُولَىٰ",
                "verse_19": "صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ"
